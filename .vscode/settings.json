{
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Shades of Purple",
    "workbench.iconTheme": "file-icons",
    "files.autoSave": "afterDelay",
    "window.zoomLevel": 1,
    "git.autofetch": true,
    "editor.fontFamily": "'Fira Code Retina',Fira Code iScript,'Operator Mono',Menlo,Monaco,Consolas, 'Courier New', monospace",
    "terminal.integrated.fontFamily": "'Fira Code Retina','Fira Code iScript',Operator Mono,Menlo,Monaco,Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.lineHeight": 24.65,
    "editor.letterSpacing": 0.5,
    "editor.renderWhitespace": "all",
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    // Formatting Optional.
    "editor.formatOnSave": true,
    "eslint.run": "onType",
    "eslint.autoFixOnSave": true,
}